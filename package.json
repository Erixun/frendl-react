{
  "name": "frendl-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "set NODE_ENV=production && tsc && vite build",
    "api:install": "cd src/api && npm install",
    "api:dev": "cd src/api && npm run dev",
    "api:build": "cd src/api && npm run build",
    "api:start": "cd src/api && npm run start",
    "all:dev": "npm-run-all --parallel dev api:dev",
    "all:build": "npm-run-all --parallel build api:build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.0.18",
    "@chakra-ui/react": "^2.5.5",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@googlemaps/js-api-loader": "^1.15.1",
    "framer-motion": "^10.11.2",
    "mobx": "^6.9.0",
    "mobx-react-lite": "^3.4.3",
    "pusher-js": "^8.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "react-toastify": "^9.1.2",
    "sass": "^1.61.0"
  },
  "devDependencies": {
    "@types/google.maps": "^3.52.5",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.9.3",
    "vite": "^4.2.0"
  }
}
